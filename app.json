{
  "expo": {
    "name": "travellblog",
    "slug": "blog-react-client",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/travelblog_icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/travel.png",
      "resizeMode": "contain",
      "backgroundColor": "#05A0A0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.apk.travellblog",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
